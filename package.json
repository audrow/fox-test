{
  "name": "system-viewer",
  "displayName": "System Viewer",
  "description": "Introspect the ROS network.",
  "publisher": "Open Robotics",
  "version": "0.0.1",
  "license": "MIT",
  "main": "./dist/extension.js",
  "scripts": {
    "build": "fox build",
    "prepare": "husky install",
    "foxglove:prepublish": "fox build --mode development",
    "local-install": "fox install",
    "lint": "eslint src --ignore-path .gitignore --max-warnings 0",
    "prettier": "prettier --ignore-path .gitignore \"**/*.(js|jsx|json|yml|yaml|css|ts|tsx|md|mdx)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "package": "fox package",
    "pretest": "fox pretest",
    "validate": "npm-run-all check-format lint build"
  },
  "devDependencies": {
    "@foxglove/eslint-plugin": "0.17.1",
    "@foxglove/fox": "0.2.1",
    "@foxglove/studio": "0.21.0",
    "@types/react": "17.0.43",
    "@types/react-dom": "17.0.14",
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "@typescript-eslint/parser": "5.3.1",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.27.0",
    "eslint-plugin-react-hooks": "4.4.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "npm-run-all": "^4.1.5",
    "prettier": "2.6.1",
    "prettier-plugin-organize-imports": "^2.3.4",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "typescript": "4.6.3"
  },
  "dependencies": {
    "@types/parse-json": "^4.0.0"
  }
}
